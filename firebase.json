{
	"firebase": "acode-benchmark-tool",
	"public": ".",
	"ignore": [
		"firebase.json",
		"**/.*",
		"**/node_modules/**"
	],
	"rules": {
		"institution-list": {
			".read": "true",
			".write": "false"
		},
		"survey": {
			".read": "true",
			".write": "true"
		}
	},
	"headers": [{
		"source" : "**",
		"headers" : [{
			"key" : "Access-Control-Allow-Origin",
			"value" : "*"
		}]
	}]
}